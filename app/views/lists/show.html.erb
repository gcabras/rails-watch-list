<h1><%=@list.name%></h1>
<ul>
<%@list.bookmarks.each do |bookmark|%>
  <li>
    <%=bookmark.movie.title%>
    <%=link_to "Delete Bookmark",
              bookmark_destroy_path(bookmark.id),
              class: "btn btn-danger",
              method: :delete,
              data: { confirm: "Are you sure you want to delete the bookmark?"} %>
  </li>
<%end%>
</ul>
<%=link_to "Add a Bookmark", new_list_bookmark_path(@list), class: "btn btn-primary"%>
<%=link_to "Go to Home Page", lists_path, class: "btn btn-primary"%>
